        </slider>
        <label lfid="sliders5" name="L20">
          <mappingparent componenttype="slider" componentname="S6" />
          <bounds x="190" y="5" width="60" height="15"/>
        </label>
        <component backgroundimage="Skins/VSliderBack.png" backgroundcolour="00364147">
          <bounds x="195" y="24" width="50" height="151" />
        </component>
        <slider lfid="sliders7" name="S20" style="linearvertical">
          <mappingparent componenttype="slider" componentname="S6" />
          <bounds x="195" y="13" width="50" height="186" />
          <textbox position="below" readonly="false" width="56" height="14">
            <font height="12"/>
