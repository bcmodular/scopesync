      </lookandfeel>
      <lookandfeel id="sliders3" parentid="sliders1">
        <colours>
          <slider thumbcolourid="ff00ff10"></slider>
        </colours>
      </lookandfeel>
      <lookandfeel id="sliders4" parentid="sliders1">
        <colours>
          <slider thumbcolourid="ff1998FF"></slider>
        </colours>
      </lookandfeel>
      <lookandfeel id="sliders5">
        <colours>
          <slider backgroundcolourid="00efeff7" thumbcolourid="ff849199" trackcolourid="ff16171a" rotarysliderfillcolourid="ffffde00" rotaryslideroutlinecolourid="2f16171a" textboxtextcolourid="ffcad5d9" textboxbackgroundcolourid="df111111" textboxhighlightcolourid="ffcad5d9" textboxoutlinecolourid="ff000000"></slider>
