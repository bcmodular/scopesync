        </slider>
        <label lfid="sliders5" name="L21">
          <mappingparent componenttype="slider" componentname="S7" />
          <bounds x="245" y="5" width="60" height="15" />
        </label>
        <slider lfid="sliders8" name="S21" style="linearbarvertical" popupenabled="true">
