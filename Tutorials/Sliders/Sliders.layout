        <colours>
          <tabbedcomponent outlinecolourid="00000000"></tabbedcomponent>
          <tabbar taboutlinecolourid="00000000" tabtextcolourid="ffc3cace" fronttextcolourid="ffff7f00"></tabbar>
        </colours>
      </lookandfeel>
