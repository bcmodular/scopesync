      <lookandfeel id="sliders1">
        <colours>
          <slider backgroundcolourid="2fefeff7" thumbcolourid="ffefeff7" trackcolourid="ff16171a" rotarysliderfillcolourid="ffffde00" rotaryslideroutlinecolourid="2f16171a" textboxtextcolourid="ff16171a" textboxbackgroundcolourid="dfefeff7" textboxhighlightcolourid="2f16171a" textboxoutlinecolourid="7f222222"></slider>
          <label textcolourid="ff16171a"></label>
          <texteditor backgroundcolourid="7f000000" textcolourid="00000000" focusedoutlinecolourid="aa555555"></texteditor>
