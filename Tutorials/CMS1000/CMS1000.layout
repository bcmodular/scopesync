        </images>
      </lookandfeel>
      <lookandfeel id="BPMKnob" parentid="maintheme">
        <appliesto componenttype="none" />
        <colours>
          <textbutton textcolouroffid="ff000000" textcolouronid="ff000000" />
        </colours>
        <images>
          <slider>
            <rotary>
              <image filename="gfx/40x40CMTempo.png" numframes="121" ishorizontal="false" />
            </rotary>
          </slider>
        </images>
      </lookandfeel>
      <lookandfeel id="PhaseKnob" parentid="maintheme">
        <appliesto componenttype="none" />
        <colours>
          <textbutton textcolouroffid="ff000000" textcolouronid="ff000000" />
        </colours>
        <images>
          <slider>
            <rotary>
              <image filename="gfx/40x40CMPhase.png" numframes="361" ishorizontal="false" />
            </rotary>
          </slider>
        </images>
      </lookandfeel>
      <lookandfeel id="0-127Knob" parentid="maintheme">
        <appliesto componenttype="none" />
        <colours>
          <textbutton textcolouroffid="ff000000" textcolouronid="ff000000" />
        </colours>
        <images>
          <slider>
            <rotary>
              <image filename="gfx/40x40CMS_0-127x4.png" numframes="128" ishorizontal="false" />
            </rotary>
          </slider>
        </images>
      </lookandfeel>
      <lookandfeel id="portaKnob" parentid="maintheme">
        <appliesto componenttype="none" />
        <colours>
          <textbutton textcolouroffid="ff000000" textcolouronid="ff000000" />
        </colours>
        <images>
          <slider>
            <rotary>
              <image filename="gfx/40x40CMS_portaTypes.png" numframes="5" ishorizontal="false" />
            </rotary>
          </slider>
        </images>
      </lookandfeel>
      <lookandfeel id="SeqDivKnob" parentid="maintheme">
        <appliesto componenttype="none" />
        <colours>
          <textbutton textcolouroffid="ff000000" textcolouronid="ff000000" />
        </colours>
        <images>
          <slider>
            <rotary>
              <image filename="gfx/40x40CMS_SeqDiv.png" numframes="3" ishorizontal="false" />
            </rotary>
          </slider>
        </images>
      </lookandfeel>
      <lookandfeel id="Seq1-8" parentid="maintheme">
        <appliesto componenttype="none" />
        <colours>
          <textbutton textcolouroffid="ff000000" textcolouronid="ff000000" />
        </colours>
        <images>
          <slider>
            <rotary>
              <image filename="gfx/40x40CMS_1-8.png" numframes="8" ishorizontal="false" />
            </rotary>
          </slider>
        </images>
      </lookandfeel>
      <lookandfeel id="bipolar24knob" parentid="maintheme">
        <appliesto componenttype="none" />
        <colours>
          <textbutton textcolouroffid="ff000000" textcolouronid="ff000000" />
        </colours>
        <images>
          <slider>
            <rotary>
              <image filename="gfx/30x30CMS_-+24Knob.png" numframes="49" ishorizontal="false" />
            </rotary>
          </slider>
        </images>
      </lookandfeel>
      <lookandfeel id="MetalKnob" parentid="maintheme">
        <appliesto componenttype="none" />
        <colours>
          <textbutton textcolouroffid="ff000000" textcolouronid="ff000000" />
        </colours>
        <images>
          <slider>
            <rotary>
              <image filename="gfx/30x30CMS_MetalKnob.png" numframes="99" ishorizontal="false" />
            </rotary>
          </slider>
        </images>
      </lookandfeel>
      <lookandfeel id="VUMeter" parentid="maintheme">
        <appliesto componenttype="none" />
        <colours>
          <textbutton textcolouroffid="ff000000" textcolouronid="ff000000" />
        </colours>
        <images>
          <slider>
            <rotary>
              <image filename="gfx/VUmeter.png" numframes="99" ishorizontal="false" />
            </rotary>
          </slider>
        </images>
      </lookandfeel>
    </lookandfeels>
    <widgettemplates>
      <!-- We set up a WidgetTemplate for our labels here, in case we want to change things like font height -->
      <!-- for all the Labels. This saves us changing throughout the layout file -->
      <label wtid="parameterlabel">
        <bounds x="0" y="0" />
        <!-- Multiple justification attributes can be supplied here if needed -->
        <justification horizontallycentred="true" />
        <font height="10" />
      </label>
    </widgettemplates>
    <!--Main Component-->
    <component backgroundimage="gfx/thumbnail_CMS3tr.png" backgroundcolour="ffffffff" lfid="maintheme">
      <backgroundimageplacement>ytop</backgroundimageplacement>
      <bounds height="720" width="1280" />
      <!--ScopeSync Header (might well want to replace with a custom header with reduced functionality)-->
      <standardcontent type="header" />
      <!--First row of controls-->
      <component backgroundcolour="44ff0000" name="First Row" id="firstrow">
        <!--Using relativerectangle for bounds makes it easy to position elements relative to others.-->
        <!--Format is left, top, right, bottom. You can reference parent (like here) or use the id for -->
        <!--the related element (must be siblings though).-->
        <bounds relativerectangle="10, 33, parent.width - 10, top + 65" />
        <!--OSC 1 controls-->
        <component backgroundcolour="4400ff00" name="OSC1" id="OSC1">
          <bounds relativerectangle="0, 0, left + 288, parent.height" />
          <rectangle outlinethickness="2" outlinecolour="ff000000" cornersize="5">
            <bounds fillparent="true" />
          </rectangle>
          <slider name="OSC1 Coarse" id="OSC1CoarseSlider" lfid="bipolar48knob">
            <bounds relativerectangle="parent.left + 5, 10, left + 40, top + 40" />
            <textbox position="none" />
          </slider>
          <label name="OSC1 Coarse" id="OSC1CoarseLabel" parametertextdisplay="shortdescription" lfid="maintheme" wtid="parameterlabel">
            <bounds relativerectangle="OSC1CoarseSlider.left, OSC1CoarseSlider.bottom + 2, OSC1CoarseSlider.right, top + 8  " />
            <!-- We always want the label to follow the mapping of the Slider, so link them together -->
            <!-- using the mappingparent element. This means we don't need to map the Labels explicitly -->
            <!-- in the ScopeSync configuration -->
            <mappingparent componentname="OSC1 Coarse" componenttype="slider" />
            <!-- For now, only size and weight of font can be specified, but this is where the font -->
            <!-- itself will be defined when that has been implemented -->
          </label>
          <slider name="OSC1 Fine" id="OSC1FineSlider" popupenabled="true" lfid="bipolar100knob">
            <bounds relativerectangle="OSC1CoarseSlider.right + 8, OSC1CoarseSlider.top, left + OSC1CoarseSlider.width, top + OSC1CoarseSlider.height" />
            <textbox position="none" />
          </slider>
          <label name="OSC1 Fine" id="OSC1FineLabel" parametertextdisplay="shortdescription" lfid="maintheme" wtid="parameterlabel">
            <bounds relativerectangle="OSC1FineSlider.left, OSC1FineSlider.bottom + 2, OSC1FineSlider.right, top + OSC1CoarseLabel.height " />
            <mappingparent componentname="OSC1 Fine" componenttype="slider" />
          </label>
          <!-- We set encodersnap="true" here, so the knob will jump between valid settings, rather than -->
          <!-- the usual continuous rotation -->
          <!--Osc1-->
          <slider name="OSC1 Wave" id="OSC1WaveSlider" popupenabled="true" encodersnap="true" lfid="wavechoiceknob" debugwidgetposition="false">
            <bounds relativerectangle="OSC1FineSlider.right + 8, OSC1CoarseSlider.top, left + OSC1CoarseSlider.width, top + OSC1CoarseSlider.height" />
            <textbox position="none" />
          </slider>
          <label name="OSC1 Wave" id="OSC1WaveLabel" parametertextdisplay="shortdescription" lfid="maintheme" wtid="parameterlabel">
            <bounds relativerectangle="OSC1WaveSlider.left, OSC1WaveSlider.bottom + 2, OSC1WaveSlider.right, top + OSC1CoarseLabel.height " />
            <mappingparent componentname="OSC1 Wave" componenttype="slider" />
          </label>
          <slider name="OSC1 Shape" id="OSC1ShapeSlider" popupenabled="true" lfid="White2DarkKnob">
            <bounds relativerectangle="OSC1WaveSlider.right + 8, OSC1CoarseSlider.top, left + OSC1CoarseSlider.width, top + OSC1CoarseSlider.height" />
            <textbox position="none" />
          </slider>
          <label name="OSC1 Shape" id="OSC1ShapeLabel" parametertextdisplay="shortdescription" lfid="maintheme" wtid="parameterlabel">
            <bounds relativerectangle="OSC1ShapeSlider.left, OSC1ShapeSlider.bottom + 2, OSC1ShapeSlider.right, top + OSC1CoarseLabel.height " />
            <mappingparent componentname="OSC1 Shape" componenttype="slider" />
          </label>
          <slider name="OSC1 WsMod1" id="OSC1WsMod1Slider" popupenabled="true" lfid="White2DarkKnob">
            <bounds relativerectangle="OSC1ShapeSlider.right + 8, OSC1CoarseSlider.top, left + OSC1CoarseSlider.width, top + OSC1CoarseSlider.height" />
            <textbox position="none" />
          </slider>
          <label name="OSC1 WsMod1" id="OSC1WsMod1Label" parametertextdisplay="shortdescription" lfid="maintheme" wtid="parameterlabel">
            <bounds relativerectangle="OSC1WsMod1Slider.left, OSC1WsMod1Slider.bottom + 2, OSC1WsMod1Slider.right, top + OSC1CoarseLabel.height " />
            <mappingparent componentname="OSC1 WsMod1" componenttype="slider" />
          </label>
          <slider name="OSC1 WsMod2" id="OSC1WsMod2Slider" popupenabled="true" lfid="White2DarkKnob">
            <bounds relativerectangle="OSC1WsMod1Slider.right + 8, OSC1CoarseSlider.top, left + OSC1CoarseSlider.width, top + OSC1CoarseSlider.height" />
            <textbox position="none" />
          </slider>
          <label name="OSC1 WsMod2" id="OSC1WsMod2Label" parametertextdisplay="shortdescription" lfid="maintheme" wtid="parameterlabel">
            <bounds relativerectangle="OSC1WsMod2Slider.left, OSC1WsMod2Slider.bottom + 2, OSC1WsMod2Slider.right, top + OSC1CoarseLabel.height " />
            <mappingparent componentname="OSC1 WsMod2" componenttype="slider" />
          </label>
        </component>
        <!--OSC 2 controls-->
        <component backgroundcolour="4400ff00" name="OSC2" id="OSC2">
          <bounds relativerectangle="OSC1.right + 8, 0, left + 285, parent.height" />
          <rectangle outlinethickness="2" outlinecolour="ff000000" cornersize="5">
            <bounds fillparent="true" />
          </rectangle>
          <slider name="OSC2 Coarse" id="OSC2CoarseSlider" popupenabled="true" lfid="bipolar48knob">
            <bounds relativerectangle="5, 10, left + 40, top + 40" />
            <textbox position="none" />
          </slider>
          <label name="OSC2 Coarse" id="OSC2CoarseLabel" parametertextdisplay="shortdescription" lfid="maintheme" wtid="parameterlabel">
            <bounds relativerectangle="OSC2CoarseSlider.left, OSC2CoarseSlider.bottom + 2, OSC2CoarseSlider.right, top + 8  " />
            <mappingparent componentname="OSC2 Coarse" componenttype="slider" />
          </label>
          <slider name="OSC2 Fine" id="OSC2FineSlider" popupenabled="true" lfid="bipolar100knob">
            <bounds relativerectangle="OSC2CoarseSlider.right + 5, 10, left + 40, top + 40" />
            <textbox position="none" />
          </slider>
          <label name="OSC2 Fine" id="OSC2FineLabel" parametertextdisplay="shortdescription" lfid="maintheme" wtid="parameterlabel">
            <bounds relativerectangle="OSC2FineSlider.left, OSC2FineSlider.bottom + 2, OSC2FineSlider.right, top + 8  " />
            <mappingparent componentname="OSC2 Coarse" componenttype="slider" />
          </label>
          <slider name="OSC2 Wave" id="OSC2WaveSlider" popupenabled="true" encodersnap="true" lfid="wavechoiceknob">
            <bounds relativerectangle="OSC2FineSlider.right + 8, OSC2CoarseSlider.top, left + OSC2CoarseSlider.width, top + OSC2CoarseSlider.height" />
            <textbox position="none" />
          </slider>
          <label name="OSC2 Wave" id="OSC2WaveLabel" parametertextdisplay="shortdescription" lfid="maintheme" wtid="parameterlabel">
            <bounds relativerectangle="OSC2WaveSlider.left, OSC2WaveSlider.bottom + 2, OSC2WaveSlider.right, top + OSC2CoarseLabel.height " />
            <mappingparent componentname="OSC2 Wave" componenttype="slider" />
          </label>
          <slider name="OSC2 Shape" id="OSC2ShapeSlider" popupenabled="true" lfid="White2DarkKnob">
            <bounds relativerectangle="OSC2WaveSlider.right + 8, OSC2CoarseSlider.top, left + OSC2CoarseSlider.width, top + OSC2CoarseSlider.height" />
            <textbox position="none" />
          </slider>
          <label name="OSC2 Shape" id="OSC2ShapeLabel" parametertextdisplay="shortdescription" lfid="maintheme" wtid="parameterlabel">
            <bounds relativerectangle="OSC2ShapeSlider.left, OSC2ShapeSlider.bottom + 2, OSC2ShapeSlider.right, top + OSC2CoarseLabel.height " />
            <mappingparent componentname="OSC2 Shape" componenttype="slider" />
          </label>
          <slider name="OSC2 WsMod1" id="OSC2WsMod1Slider" popupenabled="true" lfid="White2DarkKnob">
            <bounds relativerectangle="OSC2ShapeSlider.right + 8, OSC2CoarseSlider.top, left + OSC2CoarseSlider.width, top + OSC2CoarseSlider.height" />
            <textbox position="none" />
          </slider>
          <label name="OSC2 WsMod1" id="OSC2WsMod1Label" parametertextdisplay="shortdescription" lfid="maintheme" wtid="parameterlabel">
            <bounds relativerectangle="OSC2WsMod1Slider.left, OSC2WsMod1Slider.bottom + 2, OSC2WsMod1Slider.right, top + OSC2CoarseLabel.height " />
            <mappingparent componentname="OSC2 WsMod1" componenttype="slider" />
          </label>
          <slider name="OSC2 WsMod2" id="OSC2WsMod2Slider" popupenabled="true" lfid="White2DarkKnob">
            <bounds relativerectangle="OSC2WsMod1Slider.right + 8, OSC2CoarseSlider.top, left + OSC2CoarseSlider.width, top + OSC2CoarseSlider.height" />
            <textbox position="none" />
          </slider>
          <label name="OSC2 WsMod2" id="OSC2WsMod2Label" parametertextdisplay="shortdescription" lfid="maintheme" wtid="parameterlabel">
            <bounds relativerectangle="OSC2WsMod2Slider.left, OSC2WsMod2Slider.bottom + 2, OSC2WsMod2Slider.right, top + OSC2CoarseLabel.height " />
            <mappingparent componentname="OSC2 WsMod2" componenttype="slider" />
          </label>
        </component>
        <!--OSC 3 controls-->
        <component backgroundcolour="4400ff00" name="OSC3" id="OSC3">
          <bounds relativerectangle="OSC2.right + 8, 0, left + 88, parent.height" />
          <rectangle outlinethickness="2" outlinecolour="ff000000" cornersize="5">
            <bounds fillparent="true" />
          </rectangle>
          <slider name="OSC3 Coarse" id="OSC3CoarseSlider" popupenabled="true" lfid="bipolar48knob">
            <bounds relativerectangle="5, 10, left + 40, top + 40" />
            <textbox position="none" />
          </slider>
          <label name="OSC3 Coarse" id="OSC3CoarseLabel" parametertextdisplay="shortdescription" lfid="maintheme" wtid="parameterlabel">
            <bounds relativerectangle="OSC3CoarseSlider.left, OSC3CoarseSlider.bottom + 2, OSC3CoarseSlider.right, top + 8  " />
            <mappingparent componentname="OSC3 Coarse" componenttype="slider" />
          </label>
          <slider name="OSC3 Fine" id="OSC3FineSlider" popupenabled="true" lfid="bipolar100knob">
            <bounds relativerectangle="OSC3CoarseSlider.right + 5, 10, left + 40, top + 40" />
            <textbox position="none" />
          </slider>
          <label name="OSC3 Fine" id="OSC3FineLabel" parametertextdisplay="shortdescription" lfid="maintheme" wtid="parameterlabel">
            <bounds relativerectangle="OSC3FineSlider.left, OSC3FineSlider.bottom + 2, OSC3FineSlider.right, top + 8  " />
            <mappingparent componentname="OSC3 Fine" componenttype="slider" />
          </label>
        </component>
        <!--BpfNoise controls-->
        <component backgroundcolour="4400ff00" name="BPF" id="BPF">
          <bounds relativerectangle="OSC3.right + 8, 0, left + 78, parent.height" />
          <rectangle outlinethickness="2" outlinecolour="ff000000" cornersize="5">
            <bounds fillparent="true" />
          </rectangle>
          <slider name="Bpf Freq" id="BpfFreqSlider" popupenabled="true" lfid="white2darkknob">
            <bounds relativerectangle="18, 10, left + 40, top + 40" />
            <textbox position="none" />
          </slider>
          <label name="Bpf Freq" id="BpfFrqLabel" parametertextdisplay="shortdescription" lfid="maintheme" wtid="parameterlabel">
            <bounds relativerectangle="BpfFreqSlider.left, BpfFreqSlider.bottom + 2, BpfFreqSlider.right, top + 8  " />
            <mappingparent componentname="Bpf Freq" componenttype="slider" />
          </label>
        </component>
        <!--LFO controls-->
        <component backgroundcolour="4400ff00" name="LFO" id="LFO">
          <bounds relativerectangle="BPF.right + 8, 0, left + 438, parent.height" />
          <rectangle outlinethickness="2" outlinecolour="ff000000" cornersize="5">
            <bounds fillparent="true" />
          </rectangle>
          <textbutton id="LFOFreqSrcKnob" name="LFOFreqSrcKnob" lfid="radiolight">
            <bounds relativerectangle="5, 8, left + 50, top + 12" />
          </textbutton>
          <textbutton id="LFOFreqSrcFrqDiv" name="LFOFreqSrcFrqDiv" lfid="radiolight">
            <bounds relativerectangle="LFOFreqSrcKnob.left, LFOFreqSrcKnob.bottom + 3, left + LFOFreqSrcKnob.width, top + LFOFreqSrcKnob.height" />
          </textbutton>
          <textbutton id="LFOFreqSrcMatrix" name="LFOFreqSrcMatrix" lfid="radiolight">
            <bounds relativerectangle="LFOFreqSrcKnob.left, LFOFreqSrcFrqDiv.bottom + 3, left + LFOFreqSrcKnob.width, top + LFOFreqSrcKnob.height" />
          </textbutton>
          <label name="LFOFreqSrcLabel" id="LFOFreqSrcLabel" text="FreqSrc" lfid="maintheme" wtid="parameterlabel">
            <bounds relativerectangle="LFOFreqSrcKnob.left, LFOFreqSrcMatrix.bottom + 2, left + 38, top + 10 " />
          </label>
          <tabbedcomponent name="FreqSrcTabs" id="FreqSrcTabs">
            <bounds relativerectangle="LFOFreqSrcKnob.right - 10, 0, left + 60, parent.height" />
            <tab idx="1" name="LFOKnobTab">
              <component name="LFOKnobTabComponent" backgroundcolour="00ff0000">
                <slider name="LfoFrqSrcKnob" id="LfoFrqSrcKnobSlider" popupenabled="true" lfid="white2darkknob">
                  <bounds relativerectangle="parent.left + 5, 10, left + 40, top + 40" />
                  <textbox position="none" />
                </slider>
                <label name="LfoFrqSrcKnob" id="LfoFrqSrcKnobLabel" parametertextdisplay="shortdescription" lfid="maintheme" wtid="parameterlabel">
                  <bounds relativerectangle="LfoFrqSrcKnobSlider.left, LfoFrqSrcKnobSlider.bottom + 2, LfoFrqSrcKnobSlider.right, top + 8  " />
                  <mappingparent componentname="LfoFrqSrcKnob" componenttype="slider" />
                </label>
              </component>
            </tab>
            <tab idx="2" name="LFOFrqDivTab">
              <component name="LFOFrqDivTabComponent" backgroundcolour="0000ff00">
                <slider name="LfoFrqSrcDiv" id="LfoFrqSrcDivSlider" popupenabled="true" lfid="LfoDividerKnob">
                  <bounds relativerectangle="parent.left + 5, 10, left + 40, top + 40" />
                  <textbox position="none" />
                </slider>
                <label name="LfoFrqSrcDiv" id="LfoFrqSrcDivLabel" parametertextdisplay="shortdescription" lfid="maintheme" wtid="parameterlabel">
                  <bounds relativerectangle="LfoFrqSrcDivSlider.left, LfoFrqSrcDivSlider.bottom + 2, LfoFrqSrcDivSlider.right, top + 8  " />
                  <mappingparent componentname="LfoFrqSrcDiv" componenttype="slider" />
                </label>
              </component>
            </tab>
            <tab idx="3" name="LFOMatrixTab">
              <component name="LFOMatrixTabComponent" backgroundcolour="330000ff" />
            </tab>
          </tabbedcomponent>
          <textbutton id="LFOGateSrcMVC" name="LFOGateSrcMVC" lfid="radiolight">
            <bounds relativerectangle="110, 8, left + 50, top + 12" />
