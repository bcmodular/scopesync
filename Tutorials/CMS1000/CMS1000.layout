      </lookandfeel>
      <lookandfeel id="LfoWaveKnob" parentid="maintheme">
        <appliesto componenttype="none" />
        <colours>
          <textbutton textcolouroffid="ff000000" textcolouronid="ff000000" />
        </colours>
        <images>
          <slider>
            <rotary>
              <image filename="gfx/LfoWaveKnob.png" numframes="6" ishorizontal="false" />
            </rotary>
          </slider>
        </images>
      </lookandfeel>
      <lookandfeel id="WhiteKnob" parentid="maintheme">
        <appliesto componenttype="none" />
        <colours>
          <textbutton textcolouroffid="ff000000" textcolouronid="ff000000" />
        </colours>
        <images>
          <slider>
            <rotary>
              <image filename="gfx/40x40CMSwhite.png" numframes="99" ishorizontal="false" />
            </rotary>
