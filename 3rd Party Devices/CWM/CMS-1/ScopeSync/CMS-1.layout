<?xml version="1.0" encoding="utf-8"?>
<tns:ScopeSyncLayout xmlns:tns="http://www.scopesync.co.uk/ScopeSyncLayout" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.scopesync.co.uk/ScopeSyncLayout https://raw.githubusercontent.com/bcmodular/scopesync/master/XSD/ScopeSyncLayout.xsd ">
  <layout name="CMS-1" libraryset="CWM" author="Roy Thinnes" panelheight="684" panelwidth="1280" blurb="Layout for CMS-1" thumbnail="gfx/thumbnail_CMS3.png" excludefromchooser="false">
    <lookandfeels>
      <lookandfeel id="maintheme" parentid="system:default">
        <!--This is just to exclude the lookandfeel from Style Override options, so probably not relevant eventually-->
        <appliesto componenttype="none" />
        <colours>
          <label textcolourid="ff000000" />
          <slider textboxtextcolourid="ff000000" textboxbackgroundcolourid="ffffffff" backgroundcolourid="ffffffff" />
        </colours>
      </lookandfeel>
      <lookandfeel id="configname" parentid="maintheme">
        <colours>
          <label backgroundcolourid="00000000" textcolourid="90ffffff" />
        </colours>
      </lookandfeel>
      <!--These define styles for specific knob types (starting with bipolar100knob), which can then be referenced by-->
      <!-- specific Slider elements in the main body -->
      <lookandfeel id="bipolar100knob" parentid="maintheme">
        <appliesto componenttype="none" />
        <images>
          <slider>
            <rotary>
              <image filename="gfx/40x40CMS_-+100+.png" numframes="201" ishorizontal="false" />
            </rotary>
          </slider>
        </images>
      </lookandfeel>
      <lookandfeel id="wavechoiceknob" parentid="maintheme">
        <appliesto componenttype="none" />
        <images>
          <slider>
            <rotary>
              <image filename="gfx/40x40CMS_5OscWaves+.png" numframes="5" ishorizontal="false" />
            </rotary>
          </slider>
        </images>
      </lookandfeel>
      <lookandfeel id="bipolar48knob" parentid="maintheme">
        <appliesto componenttype="none" />
        <images>
          <slider>
            <rotary>
              <image filename="gfx/40x40CMS_-+48+.png" numframes="97" ishorizontal="false" />
            </rotary>
          </slider>
        </images>
      </lookandfeel>
      <lookandfeel id="white2darkknob" parentid="maintheme">
        <appliesto componenttype="none" />
        <images>
          <slider>
            <rotary>
              <image filename="gfx/40x40CMS_White2Darkfade+.png" numframes="99" ishorizontal="false" />
            </rotary>
          </slider>
        </images>
      </lookandfeel>
      <lookandfeel id="radiolight" parentid="maintheme">
        <appliesto componenttype="none" />
        <colours>
          <textbutton textcolouroffid="ff000000" textcolouronid="ff000000" />
        </colours>
        <images>
          <textbutton downfilename="gfx/button-on.png" upfilename="gfx/button-off.png" mouseoverdownfilename="gfx/button-on.png" mouseoverupfilename="gfx/button-off.png" />
        </images>
      </lookandfeel>
      <lookandfeel id="midilight" parentid="maintheme">
        <appliesto componenttype="none" />
        <colours>
          <textbutton textcolouroffid="ff000000" textcolouronid="ff000000" />
        </colours>
        <images>
          <textbutton downfilename="gfx/MidiActivity-On.png" upfilename="gfx/MidiActivity-Off.png" mouseoverdownfilename="gfx/MidiActivity-On.png" mouseoverupfilename="gfx/MidiActivity-Off.png" />
        </images>
      </lookandfeel>
      <lookandfeel id="LfoDividerKnob" parentid="maintheme">
        <appliesto componenttype="none" />
        <images>
          <slider>
            <rotary>
              <image filename="gfx/40x40LfoDivider.png" numframes="21" ishorizontal="false" />
