        </colours>
        <images>
          <slider>
            <rotary>
              <image filename="gfx/40x40CMS_0-127x4.png" numframes="128" ishorizontal="false" />
            </rotary>
          </slider>
        </images>
      </lookandfeel>
      <lookandfeel id="portaKnob" parentid="maintheme">
        <appliesto componenttype="none" />
        <colours>
          <textbutton textcolouroffid="ff000000" textcolouronid="ff000000" />
        </colours>
        <images>
          <slider>
            <rotary>
              <image filename="gfx/40x40CMS_portaTypes.png" numframes="5" ishorizontal="false" />
            </rotary>
          </slider>
        </images>
      </lookandfeel>
      <lookandfeel id="SeqDivKnob" parentid="maintheme">
        <appliesto componenttype="none" />
        <colours>
          <textbutton textcolouroffid="ff000000" textcolouronid="ff000000" />
        </colours>
        <images>
          <slider>
            <rotary>
              <image filename="gfx/40x40CMS_SeqDiv.png" numframes="3" ishorizontal="false" />
            </rotary>
          </slider>
        </images>
      </lookandfeel>
      <lookandfeel id="Seq1-8" parentid="maintheme">
        <appliesto componenttype="none" />
        <colours>
          <textbutton textcolouroffid="ff000000" textcolouronid="ff000000" />
        </colours>
        <images>
          <slider>
            <rotary>
              <image filename="gfx/40x40CMS_1-8.png" numframes="8" ishorizontal="false" />
            </rotary>
          </slider>
        </images>
      </lookandfeel>
      <lookandfeel id="bipolar24knob" parentid="maintheme">
        <appliesto componenttype="none" />
        <colours>
          <textbutton textcolouroffid="ff000000" textcolouronid="ff000000" />
        </colours>
        <images>
          <slider>
            <rotary>
              <image filename="gfx/30x30CMS_-+24Knob.png" numframes="49" ishorizontal="false" />
            </rotary>
          </slider>
        </images>
      </lookandfeel>
      <lookandfeel id="MetalKnob" parentid="maintheme">
        <appliesto componenttype="none" />
        <colours>
          <textbutton textcolouroffid="ff000000" textcolouronid="ff000000" />
        </colours>
        <images>
          <slider>
            <rotary>
              <image filename="gfx/30x30CMS_MetalKnob.png" numframes="99" ishorizontal="false" />
            </rotary>
          </slider>
        </images>
      </lookandfeel>
      <lookandfeel id="VUMeter" parentid="maintheme">
        <appliesto componenttype="none" />
        <colours>
          <textbutton textcolouroffid="ff000000" textcolouronid="ff000000" />
        </colours>
        <images>
          <slider>
            <rotary>
              <image filename="gfx/VUmeter.png" numframes="99" ishorizontal="false" />
            </rotary>
          </slider>
        </images>
      </lookandfeel>
      <lookandfeel id="matrixfader" parentid="maintheme">
        <appliesto componenttype="none" />
        <images>
          <slider nobackground="true">
            <linearvertical thumbfilename="gfx/Slider-ThumbBlWh.png" thumbmouseoverfilename="gfx/Slider-ThumbBlTu.png" thumbborder="0">
              <!--  <backgroundimage filename="gfx/slider-background.png" numframes="1"/>-->
            </linearvertical>
          </slider>
        </images>
        <colours>
          <slider trackcolourid="00000000" backgroundcolourid="00000000" />
        </colours>
      </lookandfeel>
      <lookandfeel id="headercontrols" parentid="maintheme">
        <appliesto componenttype="none" />
        <images>
          <slider>
            <linearhorizontal>
              <backgroundimage />
            </linearhorizontal>
          </slider>
        </images>
        <colours>
          <slider backgroundcolourid="00000000" textboxoutlinecolourid="00ffffff" textboxtextcolourid="7fffffff" />
          <label textcolourid="7fdbdbdb" outlinecolourid="7fdbdbdb" />
        </colours>
      </lookandfeel>
      <lookandfeel id="midichannelcombo" parentid="headercontrols">
        <appliesto componenttype="none" />
        <colours>
          <combobox backgroundcolourid="00000000" textcolourid="7fdbdbdb" outlinecolourid="00000000" buttoncolourid="7fdbdbdb" arrowcolourid="7fdbdbdb" focusedoutlinecolourid="00000000" />
          <label textcolourid="7fdbdbdb" outlinecolourid="00000000" />
          <popupmenu textcolourid="ffdbdbdb" backgroundcolourid="ff262d32" highlightedbackgroundcolourid="ffdbdbdb" highlightedtextcolourid="ff262d32" />
        </colours>
      </lookandfeel>
    </lookandfeels>
    <widgettemplates>
      <!-- We set up a WidgetTemplate for our labels here, in case we want to change things like font height -->
      <!-- for all the Labels. This saves us changing throughout the layout file -->
      <label wtid="parameterlabel">
        <bounds x="0" y="0" />
        <!-- Multiple justification attributes can be supplied here if needed -->
        <justification horizontallycentred="true" />
        <font height="10" />
      </label>
      <slider style="linearbar" velocitybasedmode="true" lfid="maintheme" wtid="cvbar">
        <bounds x="0" y="0" />
        <textbox readonly="false">
          <font bold="true" height="10" />
          <justification left="true" />
        </textbox>
      </slider>
      <slider style="linearvertical" encodersnap="true" inverted="true" lfid="matrixfader" wtid="matrixfader">
        <textbox position="none" />
        <bounds x="0" y="0" />
      </slider>
    </widgettemplates>
    <!--Main Component-->
    <component backgroundimage="gfx/CWMbackgrDark.png" backgroundcolour="ffffffff" lfid="maintheme">
      <backgroundimageplacement>ytop</backgroundimageplacement>
      <bounds height="746" width="1280" />
      <!--ScopeSync Header-->
      <component>
        <component id="header">
          <bounds relativerectangle="0, 1, parent.width, top + 40" />
          <textbutton lfid="system:scopesynclogo_button" name="showaboutbox" id="showaboutbox" text="" tooltip="Show About Box">
            <bounds relativerectangle="13, 11, left + 151, top + 21" />
          </textbutton>
          <label lfid="configname" name="configurationname" text="Current Configuration">
            <bounds relativerectangle="showaboutbox.right + 8, 9, left + 131, top + 17" />
            <font height="24" />
            <justification verticallycentred="true" horizontallycentred="true" />
          </label>
          <textbutton lfid="system:new_config_button" name="newconfiguration" tooltip="New Configuration">
            <bounds x="311" y="9" width="20" height="21" />
          </textbutton>
          <textbutton lfid="system:load_config_button" name="chooseconfiguration" tooltip="Load Configuration">
            <bounds x="331" y="9" width="21" height="21" />
          </textbutton>
          <textbutton lfid="system:reload_button" name="reloadconfiguration" tooltip="Reload Current Configuration">
            <bounds x="356" y="9" width="21" height="21" />
          </textbutton>
          <textbutton lfid="system:showconfigurationmanager_button" name="showconfigurationmanager" id="showconfigurationmanager" text="" tooltip="Open Configuration Manager panel">
            <bounds x="379" y="9" width="21" height="21" />
          </textbutton>
          <textbutton lfid="system:settings_button" name="showusersettings" id="showusersettings" text="" tooltip="Open User Settings panel">
            <bounds relativerectangle="right - 28, 9, parent.width - 4, top + 21" />
          </textbutton>
          <textbutton lfid="system:snapshot_button" name="snapshot" id="snapshot" text="" tooltip="Send a snapshot of all current parameter values. Ctrl-click to send snapshot for all ScopeSync instances">
            <bounds relativerectangle="right - 32, showusersettings.top, showusersettings.left - 3, top + 21" />
          </textbutton>
          <textbutton lfid="system:presets_button" name="Show Preset Window" id="showpreset" tooltip="Open Preset Browser" displaycontext="scope">
            <bounds relativerectangle="right - 19, snapshot.top, snapshot.left - 3, top + 21" />
          </textbutton>
          <label name="deviceinstance" id="deviceinstancelabel" text="Device Instance" lfid="headercontrols" displaycontext="scope">
            <bounds relativerectangle="right - 160, snapshot.top, showpreset.left - 5, snapshot.bottom" />
            <font bold="true" height="14" />
            <justification left="true" />
          </label>
          <slider name="deviceinstance" id="deviceinstanceslider" style="linearbar" nostyleoverride="true" lfid="headercontrols" displaycontext="scope">
            <bounds relativerectangle="right - 46, snapshot.top, showpreset.left - 5, snapshot.bottom" />
            <range min="0" max="1000" int="1"></range>
            <textbox position="above" readonly="false">
              <font height="14" bold="true"></font>
            </textbox>
          </slider>
          <label name="deviceinstance" id="deviceinstancelabel" text="Device Instance" lfid="headercontrols" displaycontext="host">
            <bounds relativerectangle="right - 160, snapshot.top, snapshot.left - 5, snapshot.bottom" />
            <font bold="true" height="14" />
            <justification left="true" />
          </label>
          <slider name="deviceinstance" id="deviceinstanceslider" style="linearbar" nostyleoverride="true" lfid="headercontrols" displaycontext="host">
            <bounds relativerectangle="right - 46, snapshot.top, snapshot.left - 5, snapshot.bottom" />
            <range min="0" max="1000" int="1"></range>
            <textbox position="above" readonly="false">
              <font height="14" bold="true"></font>
            </textbox>
          </slider>
          <label name="MIDI Channel" id="midichannellabel" text="MIDI Channel" lfid="headercontrols" displaycontext="scope">
            <bounds relativerectangle="right - 180, snapshot.top, deviceinstancelabel.left - 5, snapshot.bottom" />
            <font bold="true" height="14" />
            <justification left="true" />
          </label>
          <combobox name="MIDI Channel" id="midichannelcombo" nostyleoverride="true" lfid="midichannelcombo" editabletext="false" nochoicestext="-- MIDI Ch --" nothingselectedtext="" tooltip="Choose MIDI Channel for this CMS1000 instance" displaycontext="scope">
            <bounds relativerectangle="right - 66, snapshot.top, deviceinstancelabel.left - 25, snapshot.bottom" />
            <justification right="true" />
            <font bold="true" />
          </combobox>
          <textbutton name="MIDI Activity" id="midiactbutton" lfid="midilight" displaycontext="scope" tooltip="MIDI Activity">
            <bounds relativerectangle="right - 10, midichannellabel.top + (midichannellabel.height / 2) - 5, deviceinstancelabel.left - 10, top + 10" />
          </textbutton>
        </component>
      </component>
      <!--First row of controls-->
      <component backgroundcolour="00ff0000" name="First Row" id="firstrow">
        <!--Using relativerectangle for bounds makes it easy to position elements relative to others.-->
        <!--Format is left, top, right, bottom. You can reference parent (like here) or use the id for -->
        <!--the related element (must be siblings though).-->
        <bounds relativerectangle="10, 43, parent.width - 10, top + 70" />
        <!--OSC 1 controls-->
        <component backgroundcolour="00000000" name="OSC1" id="OSC1">
          <bounds relativerectangle="0, 0, left + 288, parent.height" />
          <rectangle outlinethickness="2" outlinecolour="00000000" cornersize="5" fillcolour="ffdbdbdb">
            <bounds fillparent="true" />
          </rectangle>
          <slider name="OSC1 Coarse" id="OSC1CoarseSlider" lfid="bipolar48knob">
            <bounds relativerectangle="parent.left + 5, 10, left + 40, top + 45" />
            <textbox position="none" />
          </slider>
          <label name="OSC1 Coarse" id="OSC1CoarseLabel" parametertextdisplay="shortdescription" lfid="maintheme" wtid="parameterlabel">
            <bounds relativerectangle="OSC1CoarseSlider.left, OSC1CoarseSlider.bottom + 2, OSC1CoarseSlider.right, top + 8" />
            <!-- We always want the label to follow the mapping of the Slider, so link them together -->
            <!-- using the mappingparent element. This means we don't need to map the Labels explicitly -->
            <!-- in the ScopeSync configuration -->
            <mappingparent componentname="OSC1 Coarse" componenttype="slider" />
            <!-- For now, only size and weight of font can be specified, but this is where the font -->
            <!-- itself will be defined when that has been implemented -->
          </label>
          <slider name="OSC1 Fine" id="OSC1FineSlider" popupenabled="false" lfid="bipolar100knob">
            <bounds relativerectangle="OSC1CoarseSlider.right + 8, OSC1CoarseSlider.top, left + OSC1CoarseSlider.width, top + OSC1CoarseSlider.height" />
            <textbox position="none" />
          </slider>
          <label name="OSC1 Fine" id="OSC1FineLabel" parametertextdisplay="shortdescription" lfid="maintheme" wtid="parameterlabel">
            <bounds relativerectangle="OSC1FineSlider.left, OSC1FineSlider.bottom + 2, OSC1FineSlider.right, top + OSC1CoarseLabel.height " />
            <mappingparent componentname="OSC1 Fine" componenttype="slider" />
          </label>
          <!-- We set encodersnap="true" here, so the knob will jump between valid settings, rather than -->
          <!-- the usual continuous rotation -->
          <!--Osc1-->
          <slider name="OSC1 Wave" id="OSC1WaveSlider" popupenabled="false" encodersnap="true" lfid="wavechoiceknob">
            <bounds relativerectangle="OSC1FineSlider.right + 8, OSC1CoarseSlider.top, left + OSC1CoarseSlider.width, top + OSC1CoarseSlider.height" />
            <textbox position="none" />
          </slider>
          <label name="OSC1 Wave" id="OSC1WaveLabel" parametertextdisplay="shortdescription" lfid="maintheme" wtid="parameterlabel">
            <bounds relativerectangle="OSC1WaveSlider.left, OSC1WaveSlider.bottom + 2, OSC1WaveSlider.right, top + OSC1CoarseLabel.height " />
            <mappingparent componentname="OSC1 Wave" componenttype="slider" />
          </label>
          <slider name="OSC1 Shape" id="OSC1ShapeSlider" popupenabled="false" lfid="0-127Knob">
            <bounds relativerectangle="OSC1WaveSlider.right + 8, OSC1CoarseSlider.top, left + OSC1CoarseSlider.width, top + OSC1CoarseSlider.height" />
            <textbox position="none" />
          </slider>
          <label name="OSC1 Shape" id="OSC1ShapeLabel" parametertextdisplay="shortdescription" lfid="maintheme" wtid="parameterlabel">
            <bounds relativerectangle="OSC1ShapeSlider.left, OSC1ShapeSlider.bottom + 2, OSC1ShapeSlider.right, top + OSC1CoarseLabel.height " />
            <mappingparent componentname="OSC1 Shape" componenttype="slider" />
          </label>
          <slider name="OSC1 WsMod1" id="OSC1WsMod1Slider" popupenabled="false" lfid="0-127Knob">
            <bounds relativerectangle="OSC1ShapeSlider.right + 8, OSC1CoarseSlider.top, left + OSC1CoarseSlider.width, top + OSC1CoarseSlider.height" />
            <textbox position="none" />
          </slider>
          <label name="OSC1 WsMod1" id="OSC1WsMod1Label" parametertextdisplay="shortdescription" lfid="maintheme" wtid="parameterlabel">
            <bounds relativerectangle="OSC1WsMod1Slider.left, OSC1WsMod1Slider.bottom + 2, OSC1WsMod1Slider.right, top + OSC1CoarseLabel.height " />
            <mappingparent componentname="OSC1 WsMod1" componenttype="slider" />
          </label>
          <slider name="OSC1 WsMod2" id="OSC1WsMod2Slider" popupenabled="false" lfid="0-127Knob">
            <bounds relativerectangle="OSC1WsMod1Slider.right + 8, OSC1CoarseSlider.top, left + OSC1CoarseSlider.width, top + OSC1CoarseSlider.height" />
            <textbox position="none" />
          </slider>
          <label name="OSC1 WsMod2" id="OSC1WsMod2Label" parametertextdisplay="shortdescription" lfid="maintheme" wtid="parameterlabel">
            <bounds relativerectangle="OSC1WsMod2Slider.left, OSC1WsMod2Slider.bottom + 2, OSC1WsMod2Slider.right, top + OSC1CoarseLabel.height " />
            <mappingparent componentname="OSC1 WsMod2" componenttype="slider" />
          </label>
          <component name="Osc1HeaderText" id="Osc1HeaderText" backgroundimage="gfx/ModuleHeaderOSC1_50x14.png" debugwidgetposition="false">
            <bounds relativerectangle="(parent.width / 2) - 25, 0, left + 50, top + 14" />
          </component>
        </component>
        <!--OSC 2 controls-->
        <component backgroundcolour="00000000" name="OSC2" id="OSC2">
          <bounds relativerectangle="OSC1.right + 8, 0, left + 285, parent.height" />
          <rectangle outlinethickness="2" outlinecolour="00000000" fillcolour="ffdbdbdb" cornersize="5">
            <bounds fillparent="true" />
          </rectangle>
          <slider name="OSC2 Coarse" id="OSC2CoarseSlider" popupenabled="false" lfid="bipolar48knob">
            <bounds relativerectangle="5, 10, left + 40, top + 45" />
            <textbox position="none" />
          </slider>
          <label name="OSC2 Coarse" id="OSC2CoarseLabel" parametertextdisplay="shortdescription" lfid="maintheme" wtid="parameterlabel">
            <bounds relativerectangle="OSC2CoarseSlider.left, OSC2CoarseSlider.bottom + 2, OSC2CoarseSlider.right, top + 8  " />
            <mappingparent componentname="OSC2 Coarse" componenttype="slider" />
          </label>
          <slider name="OSC2 Fine" id="OSC2FineSlider" popupenabled="false" lfid="bipolar100knob">
            <bounds relativerectangle="OSC2CoarseSlider.right + 5, OSC2CoarseSlider.top, left + OSC2CoarseSlider.width, top + OSC2CoarseSlider.height" />
            <textbox position="none" />
          </slider>
          <label name="OSC2 Fine" id="OSC2FineLabel" parametertextdisplay="shortdescription" lfid="maintheme" wtid="parameterlabel">
            <bounds relativerectangle="OSC2FineSlider.left, OSC2FineSlider.bottom + 2, OSC2FineSlider.right, top + 8  " />
            <mappingparent componentname="OSC2 Fine" componenttype="slider" />
          </label>
          <slider name="OSC2 Wave" id="OSC2WaveSlider" popupenabled="false" encodersnap="true" lfid="wavechoiceknob">
            <bounds relativerectangle="OSC2FineSlider.right + 8, OSC2CoarseSlider.top, left + OSC2CoarseSlider.width, top + OSC2CoarseSlider.height" />
            <textbox position="none" />
          </slider>
          <label name="OSC2 Wave" id="OSC2WaveLabel" parametertextdisplay="shortdescription" lfid="maintheme" wtid="parameterlabel">
            <bounds relativerectangle="OSC2WaveSlider.left, OSC2WaveSlider.bottom + 2, OSC2WaveSlider.right, top + OSC2CoarseLabel.height " />
            <mappingparent componentname="OSC2 Wave" componenttype="slider" />
          </label>
          <slider name="OSC2 Shape" id="OSC2ShapeSlider" popupenabled="false" lfid="0-127Knob">
            <bounds relativerectangle="OSC2WaveSlider.right + 8, OSC2CoarseSlider.top, left + OSC2CoarseSlider.width, top + OSC2CoarseSlider.height" />
            <textbox position="none" />
          </slider>
